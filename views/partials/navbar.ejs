<body>
    <nav>
        <% if(user){ %>
            <p>Welcome <%= user.name %></p>
            <form action="/log-out" method="POST">
                <button type="submit">Logout</button>
            </form>
        <% } else{%>
            <div class="buttons">
                <a href="/log-in">Log In</a>
                <a href="/sign-up">Sign Up</a>
            </div>
        <% } %>    
    </nav>
</body>